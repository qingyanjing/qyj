		  function showGisDeviceInfo(type, id) 
		  {
		    if(type == "SheXiangJi")
	    	{
		    	//查看视频
		    	$('.gis-panel').show('slow').removeClass('zoomIn').removeClass('zoomOut');
		    	cameraUtils.playLiveAddress("file:///D:/CloudMusic/MV/1973.mp4");
		    	cameraUtils.playCameraById(id);
	    	}
		    else if(type == "XHJ")
	    	{
		    	$('.gis-shebei-box').show().addClass('zoomIn').removeClass('zoomOut');
		    	id = id.substr(0,11);
		    	showSignalState(id,'E','','');
	    	}
/* 		    else if(type == "LeiDa")
	    	{
		    	//查看实时过车数据
		    	$('.gis-guoche-box').show().addClass('zoomIn').removeClass('zoomOut');
	    	} */
		  }
